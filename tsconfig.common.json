{
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,

		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,

		"noImplicitThis": true,
		"strict": true,

		"jsx": "preserve",
		// tsx html组件提示
		"jsxImportSource": "vue",

		"sourceMap": true,
		"isolatedModules": true,
		"esModuleInterop": true,

		"lib": ["ESNext", "DOM", "DOM.Iterable"]
	}
}
