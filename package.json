{
	"name": "@icuxika/vue-scaffold-ui",
	"author": "icuxika",
	"files": [
		"dist"
	],
	"main": "./dist/vue-scaffold-ui.umd.js",
	"module": "./dist/vue-scaffold-ui.es.js",
	"exports": {
		".": {
			"import": "./dist/vue-scaffold-ui.es.js",
			"require": "./dist/vue-scaffold-ui.umd.js"
		}
	},
	"private": false,
	"version": "0.0.1",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"preview": "vite preview",
		"lint": "eslint -c .eslintrc.json vite.config.ts \"{src/**/*,lib/**/*}.{ts,tsx,vue}\" --fix",
		"prepare": "husky install",
		"cm": "cz"
	},
	"dependencies": {
		"vue": "^3.2.25"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-conventional": "^16.2.1",
		"@commitlint/types": "^16.2.1",
		"@types/node": "^17.0.23",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"@vitejs/plugin-vue": "^2.3.0",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.6.0",
		"husky": "^7.0.0",
		"prettier": "2.6.2",
		"typescript": "^4.5.4",
		"vite": "^2.9.0",
		"vue-tsc": "^0.29.8"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
